<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>

<TabPane fx:id="tabPane" prefHeight="405.0" prefWidth="545.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="todolog.ui.MainController">
  <tabs>
    <Tab fx:id="tabTask" text="Daily Tasks"> 
         <content>
            <fx:include fx:id="paneTaskInclude" source="paneTask.fxml" />
         </content></Tab>
    <Tab fx:id="tabEntry" onSelectionChanged="#onTabEntrySelect" text="Recorded Entries">
         <content>
             <fx:include fx:id="paneEntryInclude" source="paneEntry.fxml" />
         </content></Tab>
  </tabs>
</TabPane>
